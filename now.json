{
  "version": 2,
  "target": "serverless",
  "env": {
    "AUTH0_CLIENTID": "@auth0_clientid",
    "AUTH0_SECRET": "@auth0_secret",
    "AUTH0_DOMAIN": "@auth0_domain",
    "AUTH0_COOKIE_SECRET": "@auth0_cookie_secret",
    "CS_API_URL": "@cs_api_url",
    "LA_NOTION_TOKEN": "@la_notion_token"
  },
  "build": {
    "env": {
      "AUTH0_CLIENTID": "@auth0_clientid",
      "AUTH0_SECRET": "@auth0_secret",
      "AUTH0_DOMAIN": "@auth0_domain",
      "AUTH0_COOKIE_SECRET": "@auth0_cookie_secret",
      "CS_API_URL": "@cs_api_url",
      "LA_NOTION_TOKEN": "@la_notion_token"
    }
  }
}
